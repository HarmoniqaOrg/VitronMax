# VitronMax Environment Variables Example
# Copy this file to .env and fill in appropriate values

# Core Configuration
ENV=development  # development, testing, production
LOG_LEVEL=INFO   # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Uvicorn Server Settings (defaults are usually fine for local development)
# SERVER_HOST=0.0.0.0
# SERVER_PORT=8000
# SERVER_RELOAD=True

# VitronMax Model
MODEL_PATH=models/bbb_rf_v1_0.joblib

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Supabase Storage Configuration
# Default bucket name is 'vitronmax'
# STORAGE_BUCKET_NAME=vitronmax

# OpenAI API (for explanations feature - PRD ยง3.5)
OPENAI_API_KEY=your_openai_api_key_here

# Deployment Configuration (for local manual flyctl commands)
FLY_API_TOKEN=your_fly_api_token_here

# VITE_API_BASE=https://vitronmax.fly.dev # Removed: Not used by Python backend
